# Example environment variables for Jira/Confluence Connector (do not commit real secrets)
# Copy this file to ".env" and fill values.

# Frontend origin(s) allowed for CORS (comma-separated). Use "*" for any origin (development only).
FRONTEND_ORIGIN=http://localhost:3000

# Jira OAuth 2.0
JIRA_OAUTH_CLIENT_ID=
JIRA_OAUTH_CLIENT_SECRET=
JIRA_OAUTH_REDIRECT_URI=http://localhost:3001/auth/jira/oauth/callback

# Confluence OAuth 2.0
CONFLUENCE_OAUTH_CLIENT_ID=
CONFLUENCE_OAUTH_CLIENT_SECRET=
CONFLUENCE_OAUTH_REDIRECT_URI=http://localhost:3001/auth/confluence/oauth/callback
